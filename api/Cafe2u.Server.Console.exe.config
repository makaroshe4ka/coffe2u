  <connectionStrings>
    <add name="Cafe2uContext" connectionString="data source=ДАША-ПК;initial catalog=Cafe2u;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
    <add name="Model1" connectionString="data source=WSA-076;initial catalog=Cafe2u;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
